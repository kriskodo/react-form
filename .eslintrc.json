{
  "root": true,
  "extends": [
    "airbnb",
    "airbnb/hooks"
  ],
  "rules": {
    "react/prop-types": 0,
    "react/react-in-jsx-scope": "off",
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
    "react/jsx-props-no-spreading": "off",
    "react/no-unescaped-entities": "off",
    "linebreak-style": "off",
    "no-param-reassign": "off",
    "import/no-named-default": "off"
  }
}
